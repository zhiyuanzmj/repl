{
  "name": "jsx-repl",
  "version": "0.0.1",
  "description": "JSX REPL",
  "packageManager": "pnpm@10.4.1",
  "type": "module",
  "main": "dist/ssr-stub.js",
  "module": "dist/vue-repl.js",
  "files": [
    "dist"
  ],
  "types": "dist/vue-repl.d.ts",
  "exports": {
    ".": {
      "types": "./dist/vue-repl.d.ts",
      "import": "./dist/vue-repl.js",
      "require": "./dist/ssr-stub.js"
    },
    "./monaco-editor": {
      "types": "./dist/monaco-editor.d.ts",
      "import": "./dist/monaco-editor.js",
      "require": null
    },
    "./package.json": "./package.json",
    "./style.css": "./dist/vue-repl.css",
    "./dist/style.css": "./dist/vue-repl.css"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "tag": "latest"
  },
  "scripts": {
    "dev": "vite",
    "server:dev": "nitro dev",
    "server:build": "prisma generate --no-engine && prisma migrate deploy && nitro build",
    "vercel:build": "pnpm server:build && pnpm build",
    "build": "vite build",
    "build:lib": "vite build -c vite.lib.config.ts",
    "format": "prettier --write .",
    "preview": "vite preview",
    "lint": "eslint .",
    "typecheck": "tsmc --noEmit",
    "release": "bumpp --all",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prepublishOnly": "npm run build",
    "deploy": "pnpm run build && rsync -v -a --delete ./.vercel/output/static/ root@zmjs.dev:/usr/share/nginx/html/repl"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/repl.git"
  },
  "author": "Evan You",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vuejs/repl/issues"
  },
  "homepage": "https://github.com/vuejs/repl#readme",
  "devDependencies": {
    "@babel/parser": "^7.27.2",
    "@babel/types": "^7.27.1",
    "@eslint/js": "^9.17.0",
    "@iconify-json/carbon": "^1.2.5",
    "@prisma/client": "^6.4.1",
    "@prisma/extension-accelerate": "^1.2.2",
    "@rollup/plugin-replace": "^6.0.2",
    "@shikijs/monaco": "^1.25.1",
    "@ts-macro/language-plugin": "^0.1.17",
    "@ts-macro/tsc": "^0.1.30",
    "@types/node": "^22.10.3",
    "@volar/jsdelivr": "~2.4.11",
    "@volar/monaco": "~2.4.11",
    "@vue-jsx-vapor/eslint": "^2.3.6",
    "assert": "^2.1.0",
    "bumpp": "^9.9.2",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "estree-walker": "^2.0.2",
    "fflate": "^0.8.2",
    "floating-vue": "^5.2.2",
    "globby": "^14.0.2",
    "lint-staged": "^15.3.0",
    "magic-string": "^0.30.17",
    "monaco-editor-core": "^0.52.2",
    "nitropack": "^2.10.4",
    "ofetch": "^1.4.1",
    "postcss": "^8.5.1",
    "postcss-modules": "^6.0.1",
    "prettier": "^3.4.2",
    "prisma": "^6.4.1",
    "rollup-plugin-visualizer": "^6.0.0",
    "shiki": "^1.25.1",
    "simple-git-hooks": "^2.11.1",
    "sucrase": "^3.35.0",
    "ts-macro": "^0.1.17",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0",
    "unocss": "^65.4.3",
    "unplugin-auto-import": "^19.2.0",
    "unplugin-vue-reactivity-function": "^1.1.7",
    "vite": "^6.0.6",
    "vite-plugin-dts": "^4.4.0",
    "vite-plugin-inspect": "^11.1.0",
    "volar-service-typescript": "^0.0.62",
    "vscode-uri": "^3.0.8",
    "vue": "https://pkg.pr.new/vue@46e707d",
    "vue-jsx-vapor": "^2.4.0",
    "vue-tsc": "~2.2.0"
  },
  "prisma": {
    "schema": "./server/prisma/schema.prisma"
  }
}
