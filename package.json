{
  "name": "jsx-repl",
  "version": "0.0.1",
  "description": "JSX REPL",
  "packageManager": "pnpm@10.12.4",
  "type": "module",
  "main": "dist/ssr-stub.js",
  "module": "dist/vue-repl.js",
  "files": [
    "dist"
  ],
  "types": "dist/vue-repl.d.ts",
  "exports": {
    ".": {
      "types": "./dist/vue-repl.d.ts",
      "import": "./dist/vue-repl.js",
      "require": "./dist/ssr-stub.js"
    },
    "./monaco-editor": {
      "types": "./dist/monaco-editor.d.ts",
      "import": "./dist/monaco-editor.js",
      "require": null
    },
    "./package.json": "./package.json",
    "./style.css": "./dist/vue-repl.css",
    "./dist/style.css": "./dist/vue-repl.css"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "tag": "latest"
  },
  "scripts": {
    "dev": "vite",
    "server:dev": "nitro dev",
    "server:build": "prisma generate --no-engine && prisma migrate deploy && nitro build",
    "vercel:build": "pnpm server:build && pnpm build",
    "build": "vite build",
    "build:lib": "vite build -c vite.lib.config.ts",
    "format": "prettier --write .",
    "preview": "vite preview",
    "lint": "eslint .",
    "typecheck": "tsmc --noEmit",
    "release": "bumpp --all",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prepublishOnly": "npm run build",
    "deploy": "pnpm run build && rsync -v -a --delete ./.vercel/output/static/ root@zmjs.dev:/usr/share/nginx/html/repl"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/repl.git"
  },
  "author": "Evan You",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vuejs/repl/issues"
  },
  "homepage": "https://github.com/vuejs/repl#readme",
  "devDependencies": {
    "@ampproject/remapping": "^2.3.0",
    "@babel/parser": "^7.28.0",
    "@babel/types": "^7.28.0",
    "@eslint/js": "^9.30.1",
    "@iconify-json/carbon": "^1.2.10",
    "@prisma/client": "^6.11.1",
    "@prisma/extension-accelerate": "^2.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@shikijs/monaco": "^3.7.0",
    "@ts-macro/language-plugin": "^0.2.4",
    "@ts-macro/tsc": "^0.2.4",
    "@types/node": "^22.16.0",
    "@unocss/transformer-attributify-jsx": "^66.3.3",
    "@volar/jsdelivr": "~2.4.17",
    "@volar/monaco": "~2.4.17",
    "@volar/source-map": "^2.4.17",
    "@vue-jsx-vapor/eslint": "^2.4.8",
    "assert": "^2.1.0",
    "ast-kit": "^2.1.1",
    "bumpp": "^10.2.0",
    "conventional-changelog-cli": "^5.0.0",
    "diff-match-patch-es": "^1.0.1",
    "eslint": "^9.30.1",
    "fflate": "^0.8.2",
    "floating-vue": "^5.2.2",
    "globby": "^14.1.0",
    "lint-staged": "^16.1.2",
    "magic-string": "^0.30.17",
    "monaco-editor-core": "^0.52.2",
    "nitropack": "^2.11.13",
    "ofetch": "^1.4.1",
    "path-browserify": "^1.0.1",
    "postcss": "^8.5.6",
    "postcss-modules": "^6.0.1",
    "prettier": "^3.6.2",
    "prisma": "^6.11.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "shiki": "^3.7.0",
    "simple-git-hooks": "^2.13.0",
    "source-map-js": "^1.2.1",
    "sucrase": "^3.35.0",
    "ts-macro": "^0.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "unocss": "^66.3.2",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-reactivity-function": "^1.2.0",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-inspect": "^11.3.0",
    "volar-service-typescript": "^0.0.64",
    "vscode-uri": "^3.1.0",
    "vue": "3.6.0-alpha.2",
    "vue-jsx-vapor": "^2.5.2"
  },
  "prisma": {
    "schema": "./server/prisma/schema.prisma"
  }
}
